{
  "name": "rade_portfolio",
  "version": "1.0.0",
  "description": "My portfolio website",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ignore-path ./.gitignore --fix \"./**/*.{js,ts,jsx,tsx}\"",
    "lint:check": "eslint --ignore-path ./.gitignore --quiet \"./**/*.{js,ts,jsx,tsx}\"",
    "format": "prettier --ignore-path ./.gitignore --write \"./**/*.{html,json,js,ts,jsx,tsx,css}\"",
    "format:check": "prettier --ignore-path ./.gitignore --check \"./**/*.{html,json,js,ts,jsx,tsx,css}\"",
    "gi": "git add -A && git commit -am ':octocat: :space_invader:' && git push",
    "g2": "git add -A && git commit -am ':octocat: :space_invader:'",
    "sketch": "concurrently 'npm run ts-dev' 'npm run sketch-dev'",
    "sketch-build": "rm -rf src/sketches && mkdir -p src/sketches && mv sketches/sketch1/sketch.js src/sketches/sketch.js",
    "sketch-dev": "canvas-sketch sketches/sketch1/sketch.js --hot --open",
    "ts-dev": "tsc sketches/sketch1/sketch.ts --allowUmdGlobalAccess true --esModuleInterop true --isolatedModules true --moduleResolution 'node' --target ES6 --w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rade58/portfolio.git"
  },
  "keywords": [
    "portfolio"
  ],
  "author": "Rade58",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Rade58/portfolio/issues"
  },
  "homepage": "https://github.com/Rade58/portfolio#readme",
  "dependencies": {
    "@xstate/react": "^1.0.3",
    "animejs": "^3.2.1",
    "canvas-sketch": "^0.7.4",
    "canvas-sketch-util": "^1.10.0",
    "dotenv-load": "^2.0.0",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-noise": "0.0.0",
    "glslify": "^7.1.1",
    "gsap": "^3.5.1",
    "next": "^10.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-env": "^1.1.1",
    "next-react-svg": "^1.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "theme-ui": "^0.3.4",
    "three": "^0.123.0",
    "xstate": "^4.14.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2"
  }
}
